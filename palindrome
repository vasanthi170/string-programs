#given string is palindrome or not
s   = 'malayalam'
res = ''
for ch in s:
    res = ch + res
if s == res:
    print('palindrome')
else:
    print('not a palindrome')


#given string is palindrome or not
s   = 'madam'
res = ''
for ind in range(0,len(s)//2):
    if s[ind] != s[-ind-1]:
        print('Not a Palindrome')
        break
else:
    print('palindrome')

